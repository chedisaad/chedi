<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.domotique"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.domotique.Params"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.domotique.DashBoard"
            android:label="@string/title_activity_dash_board" >
        </activity>
        <activity
            android:name="com.example.domotique.Shutter"
            android:label="@string/title_activity_shutter" >
        </activity>
        <activity
            android:name="com.example.domotique.Shutters"
            android:label="@string/title_activity_shutters" >
        </activity>
        <activity
            android:name="com.example.domotique.Ligh"
            android:label="@string/title_activity_ligh" >
        </activity>
        <activity
            android:name="com.example.domotique.Lights"
            android:label="@string/title_activity_lights" >
        </activity>
        <activity
            android:name="com.example.domotique.Rooms"
            android:label="@string/title_activity_rooms" >
        </activity>
        <activity
            android:name="com.example.domotique.Thermiques"
            android:label="@string/title_activity_thermiques" >
        </activity>
        <activity
            android:name="com.example.domotique.MaisonListActivity"
            android:label="@string/title_maison_list" >
        </activity>
        <activity
            android:name="com.example.domotique.MaisonDetailActivity"
            android:label="@string/title_maison_detail"
            android:parentActivityName=".MaisonListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MaisonListActivity" />
        </activity>
        <activity
            android:name="com.example.domotique.Maison"
            android:label="@string/title_activity_maison" >
        </activity>
        <activity
            android:name="com.example.domotique.Letter"
            android:label="@string/title_activity_letter" >
        </activity>
        <activity
            android:name="com.example.domotique.Camera"
            android:label="@string/title_activity_camera" >
        </activity>
        <activity
            android:name="com.example.domotique.DetailLetter"
            android:label="@string/title_activity_detail_letter" >
        </activity>
    </application>

</manifest>
